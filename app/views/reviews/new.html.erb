<div class="container">
  <div class="row">
    <div class="col-xs-4">
        <%= simple_form_for [@meal, @review] do |f| %>
        <%= f.input :content %>
        <%= f.input :rating, collection: (1..5).to_a, label: "Rate your experience", class: "stars" %>
        <%= f.submit "Send your review" %>
      <% end %>
    </div>
  </div>
</div>
